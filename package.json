{
  "name": "string-copier",
  "displayName": "string-copier",
  "description": "copy a string without manually highlighting the entire text",
  "version": "0.0.1",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Hocti/string-copier.git"
  },
  "publisher": "Hocti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hocti/string-copier/issues"
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
    "onCommand:string-copier.copyString"
  ],
  "contributes": {
    "commands": [
      {
        "command": "string-copier.copyString",
        "title": "Copy String"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "string-copier.copyString",
          "group": "9_cutcopypaste@5"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  }
}
